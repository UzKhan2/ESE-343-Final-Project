<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5852d0">


    <TextView
        android:id="@+id/Label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif"
        android:text="FitPeak"
        android:textColor="@color/label"
        android:textSize="34sp"
        android:textStyle="bold"
        app:flow_horizontalBias="0"
        app:flow_lastVerticalBias="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.065"
        tools:ignore="HardcodedText,TextContrastCheck" />

    <ImageView
        android:id="@+id/pushI"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:contentDescription="shows push up"
        android:src="@drawable/push"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.128"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.209"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/pushUpsBtn"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="@color/button_background_color"
        android:text="Push-Ups"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.059"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.371"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/heartI"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:contentDescription="shows heart"
        android:src="@drawable/heart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.884"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/heartBtn"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="@color/button_background_color"
        android:text="Heart Rate"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.94"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.637"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/foodI"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:contentDescription="shows plank"
        android:src="@drawable/food"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.128"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/recipeBtn"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="@color/button_background_color"
        android:text="Recipes"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.059"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.637"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/plankI"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:contentDescription="shows plank"
        android:src="@drawable/plank"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.128"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.786"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/plankBtn"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="@color/button_background_color"
        android:text="Planks"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.059"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.901"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/runningI"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:contentDescription="shows push up"
        android:src="@drawable/running"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.884"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.209"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/runningBtn"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="@color/button_background_color"
        android:text="Running"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.94"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.371"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/fatI"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:contentDescription="shows plank"
        android:src="@drawable/calc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.884"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.786"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/bmiBtn"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="@color/button_background_color"
        android:text="BMI Calculator"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.94"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.901"
        tools:ignore="HardcodedText" />

</androidx.constraintlayout.widget.ConstraintLayout>